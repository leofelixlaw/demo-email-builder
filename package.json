{
    "name": "demo-email-builder",
    "version": "1.2.2",
    "description": "MJML Componenets integration",
    "main": "dist/grapesjs-mjml.min.js",
    "scripts": {
        "lint": "eslint src",
        "v:patch": "npm version --no-git-tag-version patch",
        "build": "npm run v:patch && webpack --env.production",
        "start": "webpack-dev-server --open --progress --colors"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/leofelixlaw/demo-email-builder.git"
    },
    "babel": {
        "presets": [
            "env"
        ],
        "plugins": [
            "transform-object-rest-spread"
        ]
    },
    "dependencies": {
        "fs": "0.0.1-security",
        "grapesjs": "^0.14.62",
        "mjml": "^4.3.1",
        "mjml4-in-browser": "^1.1.1"
    },
    "devDependencies": {
        "babel-core": "^6.26.3",
        "babel-loader": "^7.1.5",
        "babel-plugin-transform-object-rest-spread": "^6.26.0",
        "babel-preset-env": "^1.7.0",
        "eslint": "^4.1.1",
        "html-webpack-plugin": "^2.30.1",
        "install-peers": "^1.0.3",
        "node-sass": "^4.12.0",
        "webpack": "^3.12.0",
        "webpack-dev-server": "^2.11.5"
    },
    "author": "",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/leofelixlaw/demo-email-builder/issues"
    },
    "homepage": "https://github.com/leofelixlaw/demo-email-builder#readme"
}
